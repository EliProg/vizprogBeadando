<Window x:Class="SchoolTimetable.UserEditWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SchoolTimetable"
        mc:Ignorable="d"
        Title="UserEditWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Label >Név</Label>
            <TextBox x:Name="name"></TextBox>
            <Label>Felhasználónév</Label>
            <TextBox></TextBox>
            <Label>Jelszó</Label>
            <TextBox></TextBox>
            <Label>Admin</Label>
            <CheckBox></CheckBox>
            <Button x:Name="Mentes" Click="Mentes_Click">Mentés</Button>
            <Button>Mégse</Button>
        </StackPanel>
    </Grid>
</Window>
